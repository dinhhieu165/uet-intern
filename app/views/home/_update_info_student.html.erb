<%= form_for @info,url: {:action => "update" } do |f| %>
 

   <div class="form-group">
      <label class="control-label col-sm-2" for="email">Email:</label>
      <div class="col-sm-10">
        
        <%= f.email_field :emailcanhan,type: "email", class:"form-control",placeholder: "Enter email" %>
      </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="email">Skype ID:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :skypeid,type: "text", class:"form-control",placeholder: "Enter skype id" %>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="email">Facebook:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :facebook,type: "text", class:"form-control",placeholder: "Enter facebook" %>
      </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="email">Dien Thoai:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :dienthoai,type: "text", class:"form-control",placeholder: "Enter phone number" %>
      </div>
    </div>

     <div class="form-group">
      <label class="control-label col-sm-2" for="email">Vi tri can bo:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :vitricanbo,type: "text", class:"form-control",placeholder: "vi tri can bo" %>
      </div>
    </div>
     <div class="form-group">
      <label class="control-label col-sm-2" for="email">Ngoai Ngu:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :ngoaingu,type: "text", class:"form-control",placeholder: "ngoai ngu" %>
      </div>
    </div>

     <div class="form-group">
      <label class="control-label col-sm-2" for="email">Chung chi:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :chungchi,type: "text", class:"form-control",placeholder: "chung chi" %>
      </div>
    </div>
     <div class="form-group">
      <label class="control-label col-sm-2" for="email">Kinh nghiem:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :kinhnghiem,type: "text", class:"form-control",placeholder: "Kinh nghiem" %>
      </div>
    </div>
     <div class="form-group">
      <label class="control-label col-sm-2" for="email">Mong muon:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :mongmuon,type: "text", class:"form-control",placeholder: "Mong muon" %>
      </div>
    </div>
     <div class="form-group">
      <label class="control-label col-sm-2" for="email">ghi chu:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :ghichu,type: "text", class:"form-control",placeholder: "ghi chu" %>
      </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="email">avatar:</label>
      <div class="col-sm-10">
        
        <%= f.file_field :avatar %>
      </div>
    </div>
    <div class="form-group">
      
      <div class="col-sm-10 col-sm-offset-2">
        
        <%= f.button :submit %>

      </div>
    </div>


<%end%>