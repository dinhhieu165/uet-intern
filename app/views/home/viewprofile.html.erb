<% if @user.has_role? :student %>
    <%= render :partial => "/home/profile_student",locals: {:user => @user} %>
<%elsif @user.has_role? :lecturer %>
    <%= render :partial => "/home/profile_lecturer",locals: {:user => @user} %>
<% elsif @user.has_role? :partner %>
    <%= render :partial => "/home/profile_partner",locals: {:user => @user} %>

<%elsif @user.has_role? :admin %>

<% end %>