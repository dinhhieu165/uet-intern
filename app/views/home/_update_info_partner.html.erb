<%= form_for @info,url: {:action => "update" } do |f| %>
 
    <div class="form-group">
      <label class="control-label col-sm-2" for="email">name:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :name,type: "text", class:"form-control",placeholder: "name" %>
      </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="email">contact:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :contact,type: "email", class:"form-control",placeholder: "contact" %>
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-2" for="email">address:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :address,type: "text", class:"form-control",placeholder: "address" %>
      </div>
    </div>



    <div class="form-group">
      <label class="control-label col-sm-2" for="email">Dien Thoai:</label>
      <div class="col-sm-10">
        
        <%= f.text_field :dienthoai,type: "text", class:"form-control",placeholder: "Enter phone number" %>
      </div>
    </div>

   

    <div class="form-group">
      <label class="control-label col-sm-2" for="email">avatar:</label>
      <div class="col-sm-10">
        
        <%= f.file_field :logo %>
      </div>
    </div>
    <div class="form-group">
      
      <div class="col-sm-10 col-sm-offset-2">
        
        <%= f.button :submit %>

      </div>
    </div>


<%end%>